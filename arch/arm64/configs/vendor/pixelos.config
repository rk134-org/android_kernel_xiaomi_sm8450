# Arch
CONFIG_ARM64_SVE=n
CONFIG_ARM64_SW_TTBR0_PAN=n
CONFIG_UNMAP_KERNEL_AT_EL0=n

# Arch - diwali
CONFIG_ARCH_DIWALI=n
CONFIG_PINCTRL_DIWALI=n
CONFIG_SM_CAMCC_DIWALI=n
CONFIG_SM_GPUCC_DIWALI=n
