# Arch
CONFIG_ARM64_SVE=n
CONFIG_ARM64_SW_TTBR0_PAN=n
CONFIG_NR_CPUS=8
CONFIG_UNMAP_KERNEL_AT_EL0=n

# Compiler
CONFIG_FRAME_WARN=0
CONFIG_CC_OPTIMIZE_FOR_PERFORMANCE_O3=y

# Coresight
CONFIG_CORESIGHT=n
CONFIG_STM=n

# Device tree
CONFIG_BUILD_ARM64_DT_OVERLAY=y

# Fingerprint
CONFIG_INPUT_FINGERPRINT=y

# Hardware ID
CONFIG_MI_HARDWARE_ID=m

# IO scheduler
CONFIG_IOSCHED_BFQ=n
CONFIG_MQ_IOSCHED_KYBER=n
CONFIG_MQ_IOSCHED_SSG=y
CONFIG_MQ_IOSCHED_SSG_CGROUP=y

# IR
CONFIG_IR_SPI=m
CONFIG_LIRC=y
CONFIG_RC_CORE=y
CONFIG_RC_DECODERS=y
CONFIG_RC_DEVICES=y

# Mtd
CONFIG_MTD=m
CONFIG_MTD_BLOCK=m
CONFIG_MTD_BLOCK2MTD=m
CONFIG_MTD_OOPS=m

# Power
CONFIG_MI_WIRELESS=y
CONFIG_BQ_FUEL_GAUGE=y

# SPSS
# CONFIG_QCOM_SPSS is not set
# CONFIG_RPMSG_QCOM_GLINK_SPSS is not set

# Thermal
CONFIG_MI_THERMAL_INTERFACE=m

# Touchscreen
CONFIG_TOUCHSCREEN_XIAOMI_TOUCHFEATURE=m

# USB
CONFIG_USB_NET_AX88179_178A=m
CONFIG_USB_NET_AX8817X=m

# Version
CONFIG_UNAME_OVERRIDE=y
CONFIG_UNAME_OVERRIDE_TARGET="com.google.android.gms"
CONFIG_UNAME_OVERRIDE_STRING="5.10.198-android12-9-00085-g226a9632f13d-ab11136126"
